#: ../auto-domain-change.php:99
#, php-format
msgid ""
"The domain name of your WordPress blog appears to have changed! <a href="
"\"%1$s\">Click here to update your config</a> or <a href=\"%2$s\">dismiss</"
"a>."
msgstr ""
"El nombre de dominio de tu blog WordPress parece haber cambiado! <a href="
"\"%1$s\">Haga clic aquí para actualizar la configuración</a> o <a href=\"%2$s"
"\">ignorar</a>."

#: ../auto-domain-change.php:114 ../auto-domain-change.php:150
msgid "Change Domain"
msgstr "Cambiar el dominio"

#: ../auto-domain-change.php:160
msgid "Change domain from: "
msgstr "Cambiar el dominio de:"

#: ../auto-domain-change.php:165
msgid "Change domain to: "
msgstr "Cambiar el dominio al:"

